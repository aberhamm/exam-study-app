# OpenRouter API Configuration (for markdown-to-json pipeline)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional: Override the default model
# OPENROUTER_MODEL=anthropic/claude-3.5-sonnet

# OpenAI API Configuration (for markdown-to-embeddings pipeline)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Override the default embedding model
# OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Optional: Override the default embedding dimensions
# EMBEDDING_DIMENSIONS=1536

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/embeddings
MONGODB_DATABASE=embeddings
